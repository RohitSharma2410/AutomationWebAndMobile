pipeline {
agent any
tools {
maven 'M3'
}
stages{



stage("test"){
steps{
sh 'mvn test'
 
}
}
    stage('Publish Cucumber Reports') {

                steps {

                    publishCucumberResults( 

                        formats: ['html', 'json'],

                        cucumberReportDir: 'target/cucumber-reports' 

                    )

                }

            
}




}




}
