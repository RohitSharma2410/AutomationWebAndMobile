pipeline {
agent any
tools {
maven 'M3'
}
stages{



stage("test"){
steps{
sh 'mvn test'
 
}
}
      stage('Publish Cucumber Report') {

            steps {

                publishCucumberReports(reportsDirectory: 'target/cucumber-report', 

                                    reportName: 'Cucumber Report', 

                                    reportTitle: 'Cucumber Test Results')

            }

        }




}




}
