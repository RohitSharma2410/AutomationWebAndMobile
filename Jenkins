pipeline {
agent any
tools {
maven 'M3'
}
stages{



stage("test"){
steps{
sh 'mvn test'
 publishHTML (target : [allowMissing: false,
 alwaysLinkToLastBuild: true,
 keepAll: true,
 reportDir: 'reports',
 reportFiles: 'myreport.html',
 reportName: 'My Reports',
 reportTitles: 'The Report'])
}
}
    




}




}
